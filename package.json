  "description": "keeps your Heroku dynos awake",
  "main": "src/index.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "test": "jest",
    "test-coverage": "npm run test -- --coverage",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "lint-fix": "eslint --fix './src/**/*.{js,jsx,ts,tsx}'",
    "deploy": "npm publish",
    "deploy-beta": "npm publish --tag beta",
    "dev": "node src/demo.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcinxkaminski/heroku-caffeine.git"
  },
  "keywords": [
