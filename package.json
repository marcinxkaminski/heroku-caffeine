    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^21.0.0",
    "jest": "^26.4.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
